[user]
    name = Alexey Rybakov
    email = alexrybakov@yandex.ru
[core]
    editor = vim
[merge]
    tool = vimdiff
[push]
    default = current
[alias]
    st = status
    sa = stash apply
    sp = stash pop
    df = diff
    dc = diff --cached
    cm = commit -m
    cam = commit -am
    co = checkout
    me = merge
    pu = push
    pl = pull
    pb = pull --rebase
    re = reset
    rh = reset HEAD
    rb = rebase
[color]
    ui = auto
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
