[user]
    name = Alexey Rybakov
    email = alexrybakov@yandex.ru
[core]
    editor = vim
[merge]
    tool = vimdiff
[push]
    default = current
[alias]
    st = status
    b = branch
    bd = branch -D
    df = diff
    dc = diff --cached
    a = add
    aa = add -A
    cm = commit -m
    cam = commit -am
    amend = commit --amend
    co = checkout
    fo = fetch origin
    todev = "!git fo && git co dev && git pull"
    rbdev = "!git fo && git rb origin/dev"
    fresh = clean -xdf
    pl = pull
    plr = pull --rebase
    pu = push
    rb = rebase
    rbcont = rebase --continue
    rbskip = rebase --skip
    rb2 = rebase -i HEAD~2
    rb3 = rebase -i HEAD~3
    rb4 = rebase -i HEAD~4
    rb5 = rebase -i HEAD~5
    rb6 = rebase -i HEAD~6
    last = show HEAD
[color]
    ui = auto
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
